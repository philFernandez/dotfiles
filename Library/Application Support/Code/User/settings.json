{
    "java.inlayHints.parameterNames.enabled": "none",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    // * EDITOR SETTINGS
    "editor.fontFamily": "VictorMono Nerd Font",
    "editor.suggestSelection": "first",
    "editor.fontSize": 15.7,
    "editor.bracketPairColorization.enabled": true,
    "editor.fontLigatures": true,
    "terminal.integrated.fontSize": 15.4,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.defaultProfile.osx": "zshnl",
    // ! This (particularly the zshnl value) prevents zsh from being a login shell. Setting zshnl as the default terminal 
    // ! (cmd+p -> Terminal: Select Default Profile) prevents $PATH from being loaded twice when opening with the `code .` 
    // ! terminal command. The drawback is that now $ZDOTDIR/.zlogin is not sourced at all when opening vscode any other 
    // ! way that using the `code .` command, and hence $PATH isn't fully loaded. So only open vscode with `code .` 
    // ! (or you can run zsh -l manually from the terminal if opening other than with `code .`).
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh",
            "args": [
                "-l"
            ]
        },
        "zshnl": {
            "path": "/bin/zsh"
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "workbench.productIconTheme": "developer-icons",
    "workbench.colorTheme": "Bearded Theme Arc",
    // "workbench.colorTheme": "Bearded Theme Monokai Reversed",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": false,
        "yaml": false,
        "plaintext": false,
        "markdown": false,
        "java": false
    },
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    // * PEACOCK COLORS
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#dd0531"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#61dafb"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Svelte Orange",
            "value": "#ff3d00"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        },
        {
            "name": "Philthy Dark",
            "value": "#363636"
        }
    ],
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "info",
        "warning"
    ],
    "explorer.confirmDelete": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "javascript.format.semicolons": "insert",
    "vim.foldfix": true,
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "/Users/philthy/.config/vscode_vim/vimrc",
    "vim.searchHighlightColor": "yellow",
    "vim.searchHighlightTextColor": "black",
    "vim.hlsearch": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgba(250, 240, 170, 0.5)",
    "vim.highlightedyank.duration": 400,
    "vim.leader": ",",
    "vim.showMarksInGutter": true, // :delmarks! to delete all marks
    "cSpell.userWords": [
